{
    "name": "jdmapp",
    "version": "1.1.0",
    "description": "application for jdm - jeudemot -",
    "scripts": {
        "start": "node ./jdm-server/app.js",
        "client": "cd jdm-client && yarn start",
        "server": "cd jdm-server && yarn dev",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "heroku-postbuild": "cd jdm-client && yarn install && yarn install --only=dev --no-shrinkwrap && yarn build "
    },
    "author": "per5u5",
    "license": "ISC",
    "dependencies": {
        "react-loader-spinner": "^4.0.0",
        "react-redux": "^7.2.2",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "concurrently": "^5.3.0",
        "debug": "^4.3.1"
    }
}
